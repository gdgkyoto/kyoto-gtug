<?xml version="1.0" encoding="UTF-8" ?>
<Module>
  <ModulePrefs title="Hello Wave" height="300">
    <Require feature="wave" />
  </ModulePrefs>
  <Content type="html">
    <![CDATA[
    <link type="text/css" href="http://mutetheradio.no-ip.org/mtr-wave/css/smoothness/jquery-ui.css" rel="stylesheet" /> 
    <link type="text/css" href="http://mutetheradio.no-ip.org/mtr-wave/css/mtr.css" rel="stylesheet" /> 
    <script type="text/javascript">
      var SM2_DEFER = true;
    </script>
    <script type="text/javascript" src="http://mutetheradio.no-ip.org/mtr-wave/js/jquery.js"></script>
    <script type="text/javascript" src="http://mutetheradio.no-ip.org/mtr-wave/js/jquery-ui.js"></script>
    <script type="text/javascript" src="http://mutetheradio.no-ip.org/mtr-wave/js/soundmanager2.js"></script>
    <script type="text/javascript" src="http://mutetheradio.no-ip.org/mtr-wave/js/mtr3.js"></script>
    <div id="tempo-l">Tempo <span id="tempo_disp" class="slider_disp"></span></div>
    <div id="tempo-i" class="inner"><br><div id="tempo" class="slider"></div><br></div>
    <div id="step-l">Step <span id="step_disp" class="slider_disp"></span></div>
    <div id="step-i" class="inner"><br><div id="step" class="slider"></div><br></div>
    <div id="bar-l">Bar <span id="bar_disp" class="slider_disp"></span></div>
    <div id="bar-i" class="inner"><br><div id="bar" class="slider"></div><br></div>
    <input id="play" type="button" value="Play"/>
    <input id="stop" type="button" value="Stop"/>
    <input id="init" type="button" value="Init"/>
    <div style="clear:both;"></div>
    <div style="float:left;" class="sample">clash<br/>open-hh<br/>close-hh <br/>snare<br/>kick</div>
    <div id="seq"></div>
    <br/>
    <div>Javascript Drum Machine.</div>
    ]]>
  </Content>
</Module>
