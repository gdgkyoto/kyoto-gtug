<?xml version="1.0" encoding="UTF-8" ?>
<Module>
  <ModulePrefs title="Hello Wave" width="640" height="480">
    <Require feature="wave" />
  </ModulePrefs>
  <Content type="html">
    <![CDATA[
    <link type="text/css" href="css/smoothness/jquery-ui.css" rel="stylesheet" /> 
    <link type="text/css" href="css/mtr.css" rel="stylesheet" />
    <script type="text/javascript">
      var SM2_DEFER = true;
      var SERVER_PATH="http://localhost/";//ローカルで処理するときは以下の行をコメントアウト。
      var SERVER_PATH="http://kyoto-gtug.googlecode.com/svn/trunk/GoogleWave20091226/multimedia/";
    </script>
    <script type="text/javascript" src="http://kyoto-gtug.googlecode.com/svn/trunk/GoogleWave20091226/multimedia/js/jquery.js"></script>
    <script type="text/javascript" src="http://kyoto-gtug.googlecode.com/svn/trunk/GoogleWave20091226/multimedia/js/jquery-ui.js"></script>
    <script type="text/javascript" src="http://kyoto-gtug.googlecode.com/svn/trunk/GoogleWave20091226/multimedia/js/soundmanager2.js"></script>
    <script type="text/javascript" src="http://kyoto-gtug.googlecode.com/svn/trunk/GoogleWave20091226/multimedia/js/json2.js"></script>
    <script type="text/javascript" src="http://kyoto-gtug.googlecode.com/svn/trunk/GoogleWave20091226/multimedia/js/initSoundManager.js"></script>
    <script type="text/javascript" src="http://kyoto-gtug.googlecode.com/svn/trunk/GoogleWave20091226/multimedia/js/processing.js"></script>
    <script type="text/javascript" src="http://kyoto-gtug.googlecode.com/svn/trunk/GoogleWave20091226/multimedia/js/model.js"></script>
    <script type="text/javascript">
      gadgets.util.registerOnLoadHandler(init);
    </script>
    
    <canvas id="tempcanvas" width="140" height="140"></canvas>
    <div id="tempo-l">Tempo <span id="tempo_disp" class="slider_disp"></span></div>
    <div id="tempo-i" class="inner"><br><div id="tempo" class="slider"></div><br></div>
    <input id="play" type="button" value="Play"/>
    <input id="playSong" type="button" value="PlaySong"/>
    <input id="stop" type="button" value="Stop"/>
    <input id="init" type="button" value="Init"/>
    <div style="clear:both;"></div>
    <div id="seq"></div>
    <image id="p-icon" src="http://kyoto-gtug.googlecode.com/svn/trunk/GoogleWave20091226/multimedia/image/piano.png"></image>
    <image id="g-icon" src="http://kyoto-gtug.googlecode.com/svn/trunk/GoogleWave20091226/multimedia/image/guitar.png"></image>
    <image id="b-icon" src="http://kyoto-gtug.googlecode.com/svn/trunk/GoogleWave20091226/multimedia/image/bass.png"></image>
    <image id="d-icon" src="http://kyoto-gtug.googlecode.com/svn/trunk/GoogleWave20091226/multimedia/image/drum.png"></image>
    <div class="icon" id="piano">TEST1</div>
    <div class="icon" id="guitar" style="display:none">TEST2</div>
    <div class="icon" id="bass" style="display:none">TEST3</div>
    <div class="icon" id="drum" style="display:none">TEST4</div>
    <script type="text/javascript">
      onload = function() {
  draw();
  var canvas = document.getElementsById('tempcanvas');
  var p = Processing(canvas);
  p.rect(10,10,70,70);
};
function draw() {
  /* canvas要素のノードオブジェクト */
  var canvas = document.getElementById('tempcanvas');
  /* canvas要素の存在チェックとCanvas未対応ブラウザの対処 */
  if ( ! canvas || ! canvas.getContext ) {
    return false;
  }
  /* 2Dコンテキスト */
  var ctx = canvas.getContext('2d');
  /* 四角を描く */
  ctx.beginPath();
  ctx.moveTo(20, 20);
  ctx.lineTo(120, 20);
  ctx.lineTo(120, 120);
  ctx.lineTo(20, 120);
  ctx.closePath();
  ctx.stroke();
}
    </script>
    ]]>
  </Content>
</Module>
